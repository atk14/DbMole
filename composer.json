{
    "name": "atk14/dbmole",
    "description": "Database layer library for Atk14 framework",
    "type": "library",
    "homepage": "https://github.com/atk14/DbMole",
    "license": "MIT",
    "require": {
			"atk14/files": ">1.3",
			"atk14/sendmail": "1.*"
		},
		"require-dev": {
			"atk14/stop-watch": "0.*",
			"atk14/files": ">1.3",
			"atk14/tester": "*"
		},
		"autoload": {
			"classmap": ["src"]
		},
		"autoload-dev": {
			"classmap": ["src","test"]
		}
}
